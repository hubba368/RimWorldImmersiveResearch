      </li>
    </ingredients>
    <products>
      <FinishedExperiment>1</FinishedExperiment>
    </products>
    <fixedIngredientFilter>
      <thingDefs>
        <li>FinishedExperiment</li>
      </thingDefs>
    </fixedIngredientFilter>
    <modExtensions>
      <li Class="ImmersiveResearch.ResearchDefModExtension">
        <researchTypes>
          <li>Mechanical</li>
        </researchTypes>
        <ResearchSize>
          Small
        </ResearchSize>
      </li>
    </modExtensions>
  </RecipeDef>

